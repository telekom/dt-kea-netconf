{
    "avail": "1.5.0",
    "brief": [
        "This command retrieves all IPv6 leases by page."
    ],
    "cmd-comment": [
        "The from address and the page size limit are mandatory."
    ],
    "cmd-syntax": [
        "{",
        "    \"command\": \"lease6-get-page\",",
        "    \"arguments\": {",
        "        \"limit\": <integer>,",
        "        \"from\": <IPv6 address or start>",
        "    }",
        "}"
    ],
    "hook": "lease_cmds",
    "name": "lease6-get-page",
    "support": [
        "kea-dhcp6"
    ]
}
